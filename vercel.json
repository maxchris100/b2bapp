{
    "version": 2,
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/client/dist/$1"
        },
        {
            "src": "/api/trpc/(.*)",
            "dest": "/index.js"
        }
    ]
}