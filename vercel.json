{
    "projects": {
        "server": {
            "framework": null
        }
    },
    "builds": [
        {
            "src": "index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/client/dist/assets/$1"
        },
        {
            "src": "/",
            "dest": "/client/dist/index.html"
        },
        {
            "src": "/api/(.*)",
            "dest": "/server/index.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/client/dist/index.html"
        }
    ]
}